{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "preserve",
    "baseUrl": ".",
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "noUnusedLocals": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["env.d.ts", "src/**/*", ".vitepress/**/*", ".vitepress/theme/types/**/*"]
}
